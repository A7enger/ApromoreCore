<?xml version="1.0" encoding="utf-8"?>
<config>
    <!-- List of plugins. One plugin is one class. -->
    <!-- Each plugin class can register one or more toolbar buttons. The button group must belong to one of the groups below -->
	<plugins>
		<plugin source="toolbar.js" name="Toolbar" />
		<plugin source="undo.js" name="Undo" />
		<plugin source="apromoreSave.js" name="ApromoreSave"/>
        <plugin source="export.js" name="Export"/>
		<plugin source="pdf.js" name="File"/>
		<plugin source="view.js" name="View"/>
		<plugin source="share.js" name="Share"/>
		<plugin source="simulationPanel.js" name="SimulationPanel"/>
	</plugins>

    <!-- Order of button groups on the toolbar -->
	<properties>
		<property group="File" index="1" />
		<property group="View" index="2" />
		<property group="Undo" index="3" />
		<property group="Zoom" index="4" />
		<property group="Simulation" index="5" />
        <property group="Share" index="6" />
	</properties>
</config>
