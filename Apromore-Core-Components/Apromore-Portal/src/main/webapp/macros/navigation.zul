<window id="treeW" hflex="true" vflex="true">
    <style>
        .h-inline-block {
        display: inline-block;
        _display: inline;
        }
    </style>
    <div class="z-west-header" style="display: -webkit-flex; display: flex">
        <label style="color: unset; font-weight: bold; -webkit-flex: 1; flex: 1" value="Folders"/>
        <toolbarbutton id="expand" image="/img/expandall.gif" />
        <toolbarbutton id="contract" image="/img/collapseall.gif" />
