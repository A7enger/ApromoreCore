<window style="z-index: 40000 !important" closable="true" id="matchedMappingWindow" maximizable="false" hflex="min"
        vflex="min" height="230px"
        width="672px"
        position="center" title="Matched schema mapping found!">
    <style src="/themes/ap/csv-importer/css/index.css"/>
    <style src="/themes/ap/common/css/utils.css"/>
    <!--<style>-->
        <!--.z-modal-mask{-->
        <!--z-index:1!important;-->
        <!--}-->
    <!--</style>-->
    <borderlayout>
        <center sclass="ap-csv-importer-main">
            <div style="padding: 8px">
                <label value=" An existing csv schema mapping is applicable to this log. Would you like to use it? "/>
                <separator/>
                <separator/>
                <label value="Select 'Yes' to upload this log using this existing csv schema mapping."/>
                <separator/>
                <label value="Select 'Edit' to load this existing csv schema mapping."/>
                <separator/>
                <label value="Select 'No' to upload this log as a new process."/>
                <separator/>

                <separator/>
            </div>
        </center>

        <south sclass="ap-csv-importer-footer" hflex="min" style="border:0; border-top-width: 1px">
            <div style="padding: 8px">
                <hbox spacing="0" align="center" pack="center" hflex="1" vflex="1">
                    <div hflex="1" style="text-align: center;">
                        <button id="uploadWithMatchedMapping" label="Yes"
                                iconSclass="z-icon-arrow-circle-down fa-lg z-icon-fw" style="position:relative;"/>

                        <button id="editWithMatchedMapping" label="Edit"
                                iconSclass="z-icon-check-circle fa-lg z-icon-fw" style="position:relative;"/>

                        <button id="uploadAsNew" label="No" visible="false"
                                iconSclass="z-icon-check-circle fa-lg z-icon-fw" style="position:relative;"/>

                    </div>
                    <separator/>
                </hbox>
            </div>
        </south>
    </borderlayout>
</window>
