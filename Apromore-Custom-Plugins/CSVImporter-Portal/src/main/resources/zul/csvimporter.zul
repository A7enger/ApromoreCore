<zk>

    <window closable="true" height="min" width="min" position="center" title="CSV Importer">
        <!--<vbox>-->
        <style>
            .greenLabel {
            font-family: sans-serif;
            color: green;
            font-size: 11px;
            font-weight: normal;
            margin-left: -3px;
            }

            .redLabel {
            font-family: sans-serif;
            color: red;
            font-size: 11px;
            font-weight: normal;
            margin-left: -3px;
            }

            .z-row-content, .z-group-content, .z-groupfoot-content, .z-footer-content {
            font-family: Arial,Sans-serif;
            font-size: 12px;
            color: #636363;
            padding: 0px 0px;
            line-height: 12px;
            overflow: hidden;
            width: max-content;
            }

            .myGroupBox {
            display: block;
            height: inherit;
            border: 1px solid #cfcfcf;
            border-top: 0;
            padding: 2px;
            overflow: auto;
            zoom: 1;
            width: 100%;
            height: 95%;
            }
            .relativePos{
            position:relative;
            width: 100%;
            height: 57px;
            }

            tr.z-row td.z-row-inner, tr.z-row .z-cell {
            <!--line-height: 25px;-->
            padding-left: 10px;
            }
        </style>

        <div id="gridBox" zclass="myGroupBox">
            <button id="toXESButton" label="Import Log" image="img/ok.png" disabled="true" style="position:absolute;"></button>
            <separator />
            <div id="popUPBox" zclass="relativePos"></div>
            <div id="attrBox"></div>
            <grid id="myGrid" mold="paging" hflex="min" vflex="min" />
        </div>

        <!--<groupbox>-->
            <!--<hbox align="center">-->
                <!--<button id="toXESButton" label="Import Log" image="img/ok.png" disabled="true"></button>-->
                <!--<separator />-->
            <!--</hbox>-->
        <!--</groupbox>-->

    </window>
</zk>