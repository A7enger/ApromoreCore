                <div id="gridBox" zclass="myGroupBox">
                    <div id="popUPBox" zclass="relativePos"></div>
                    <div zclass="banner" />
                    <!--<div id="attrBox" visible="false"></div>-->
                    <grid id="myGrid" mold="default" hflex="min" height="600px" onClientInfo="onClientInfo(event)" style="padding-top: 60px;overflow: auto;">
                        <zscript>
                            void onClientInfo(ClientInfoEvent evt) {
                                int height = evt.getDesktopHeight();
                                int width = evt.getDesktopWidth();
                                myGrid.setHeight(height/1.15 + "px");
                            }
                        </zscript>
                    </grid>
                </div>
